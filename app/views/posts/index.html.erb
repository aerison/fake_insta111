<a href="/posts/new">글 쓰러가기</a>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Instead of user, id</th>
      <td scope="col">Comments</td>
    </tr>
  </thead>
  <tbody>
        <% @posts.each do |post| %>
        <tr>
          <th scope="row"><%= post.id %></th>
          <td><a href="/posts/<%= post.id %>"><%= post.title %></a></td>
        </tr>
        <% end %>
  </tbody>
</table>
